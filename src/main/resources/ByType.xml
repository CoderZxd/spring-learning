<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="textEditor" class="com.zxd.spring.test.learning.autowired.bytype.TextEditor" autowire="byType">
        <property name="name" value="Generic Text Editor"></property>
    </bean>
    <bean id="spellChecker" class="com.zxd.spring.test.learning.autowired.bytype.SpellChecker"></bean>
</beans>