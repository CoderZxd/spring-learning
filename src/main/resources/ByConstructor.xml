<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="textEditor" class="com.zxd.spring.test.learning.autowired.byconstructor.TextEditor" autowire="constructor">
        <constructor-arg name="name" value="Generic Text Editor"></constructor-arg>
    </bean>
    <bean id="spellChecker" class="com.zxd.spring.test.learning.autowired.byconstructor.SpellChecker"></bean>
</beans>